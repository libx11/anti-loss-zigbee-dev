        RSEG BANKED_CODE:CODE:NOROOT(0)
//  227 void InitT1(void)
InitT1:
        CFI Block cfiBlock0 Using cfiCommon0
        CFI Function InitT1
        CODE
//  228 {
        PUSH    DPL
        CFI DPL0 Frame(CFA_SP, 4)
        CFI CFA_SP SP+-4
        PUSH    DPH
        CFI DPH0 Frame(CFA_SP, 5)
        CFI CFA_SP SP+-5
        ; Saved register size: 2
        ; Auto size: 0
//  229     CLKCONCMD &= ~0x40;      //设置系统时钟源为32MHZ晶振
        ANL     0xc6,#0xbf
//  230     while(CLKCONSTA & 0x40); //等待晶振稳定为32M
??InitT1_0:
        MOV     A,0x9e
        MOV     C,0xE0 /* A   */.6
        JC      ??InitT1_0
//  231     CLKCONCMD &= ~0x07;      //设置系统主时钟频率为32MHZ   
        ANL     0xc6,#0xf8
//  232     CLKCONCMD |= 0x38;       //时钟速度32 MHz 定时器标记输出设置[5:3]250kHz
        ORL     0xc6,#0x38
//  233 
//  234     PERCFG |= 0x40;          //定时器1 的IO位置   1:备用位置2 
        ORL     0xf1,#0x40
//  235     P2SEL &= ~0x10;          //定时器1优先
        ANL     0xf5,#0xef
//  236     P2DIR |= 0xC0;           //第1优先级：定时器1通道2-3
        ORL     0xff,#0xc0
//  237 
//  238     P0DIR |= 0x80;           //端口1为输出    
        ORL     0xfd,#0x80
//  239     P0SEL |= 0x80;           //timer1 通道2映射口P1_0
        ORL     0xf3,#0x80
