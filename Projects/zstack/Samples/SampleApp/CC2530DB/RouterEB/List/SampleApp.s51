//  206 #define BEEP P0_7  //蜂鸣器；
//  207 
//  208 
//  209 
//  210 /****************************************************************************
//  211 * 名    称: InitLed()
//  212 * 功    能: 设置LED灯相应的IO口
//  213 * 入口参数: 无
//  214 * 出口参数: 无
//  215 ****************************************************************************/
//  216 
//  217 /****************************************************************************
//  218 * 名    称: InitT1()
//  219 * 功    能: 定时器初始化，TICKSPD 是16 MHz系统不配置时默认是2分频，即16MHz
//  220 * 入口参数: 无
//  221 * 出口参数: 无
//  222 ****************************************************************************/
//  223 
//  224 

        RSEG BANKED_CODE:CODE:NOROOT(0)
//  225 void InitT1(void)
InitT1:
        CFI Block cfiBlock0 Using cfiCommon0
        CFI Function InitT1
        CODE
//  226 {
        PUSH    DPL
        CFI DPL0 Frame(CFA_SP, 4)
        CFI CFA_SP SP+-4
        PUSH    DPH
        CFI DPH0 Frame(CFA_SP, 5)
        CFI CFA_SP SP+-5
        ; Saved register size: 2
        ; Auto size: 0
//  227     CLKCONCMD &= ~0x40;      //设置系统时钟源为32MHZ晶振
        ANL     0xc6,#0xbf
//  228     while(CLKCONSTA & 0x40); //等待晶振稳定为32M
??InitT1_0:
        MOV     A,0x9e
        MOV     C,0xE0 /* A   */.6
        JC      ??InitT1_0
//  229     CLKCONCMD &= ~0x07;      //设置系统主时钟频率为32MHZ   
        ANL     0xc6,#0xf8
//  230     CLKCONCMD |= 0x38;       //时钟速度32 MHz 定时器标记输出设置[5:3]250kHz
        ORL     0xc6,#0x38
//  231 
//  232     PERCFG |= 0x40;          //定时器1 的IO位置   1:备用位置2 
        ORL     0xf1,#0x40
//  233     P2SEL &= ~0x10;          //定时器1优先
        ANL     0xf5,#0xef
//  234     P2DIR |= 0xC0;           //第1优先级：定时器1通道2-3
        ORL     0xff,#0xc0
//  235 
//  236     P0DIR |= 0x80;           //端口1为输出    
        ORL     0xfd,#0x80
//  237     P0SEL |= 0x80;           //timer1 通道2映射口P1_0
        ORL     0xf3,#0x80
//  238     
//  239     T1CC3H = 0x00;           //20%占空比为200us
        MOV     DPTR,#0x62ad
        CLR     A
        MOVX    @DPTR,A
//  240     T1CC3L = 0x00;           //修改T1CC2L可调整led的亮度
        MOV     DPTR,#0x62ac
