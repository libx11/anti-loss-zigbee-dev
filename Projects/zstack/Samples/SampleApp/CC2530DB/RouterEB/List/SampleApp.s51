//  203 #define BEEP P0_7  //蜂鸣器；
//  204 
//  205 
//  206 
//  207 /****************************************************************************
//  208 * 名    称: InitLed()
//  209 * 功    能: 设置LED灯相应的IO口
//  210 * 入口参数: 无
//  211 * 出口参数: 无
//  212 ****************************************************************************/
//  213 
//  214 /****************************************************************************
//  215 * 名    称: InitT1()
//  216 * 功    能: 定时器初始化，TICKSPD 是16 MHz系统不配置时默认是2分频，即16MHz
//  217 * 入口参数: 无
//  218 * 出口参数: 无
//  219 ****************************************************************************/
//  220 
//  221 

        RSEG BANKED_CODE:CODE:NOROOT(0)
//  222 void InitT1(void)
InitT1:
        CFI Block cfiBlock0 Using cfiCommon0
        CFI Function InitT1
        CODE
//  223 {
        PUSH    DPL
        CFI DPL0 Frame(CFA_SP, 4)
        CFI CFA_SP SP+-4
        PUSH    DPH
        CFI DPH0 Frame(CFA_SP, 5)
        CFI CFA_SP SP+-5
        ; Saved register size: 2
        ; Auto size: 0
//  224     CLKCONCMD &= ~0x40;      //设置系统时钟源为32MHZ晶振
        ANL     0xc6,#0xbf
//  225     while(CLKCONSTA & 0x40); //等待晶振稳定为32M
??InitT1_0:
        MOV     A,0x9e
        MOV     C,0xE0 /* A   */.6
        JC      ??InitT1_0
//  226     CLKCONCMD &= ~0x07;      //设置系统主时钟频率为32MHZ   
        ANL     0xc6,#0xf8
//  227     CLKCONCMD |= 0x38;       //时钟速度32 MHz 定时器标记输出设置[5:3]250kHz
        ORL     0xc6,#0x38
//  228 
//  229     PERCFG |= 0x40;          //定时器1 的IO位置   1:备用位置2 
        ORL     0xf1,#0x40
//  230     P2SEL &= ~0x10;          //定时器1优先
        ANL     0xf5,#0xef
//  231     P2DIR |= 0xC0;           //第1优先级：定时器1通道2-3
        ORL     0xff,#0xc0
//  232 
//  233     P0DIR |= 0x80;           //端口1为输出    
        ORL     0xfd,#0x80
//  234     P0SEL |= 0x80;           //timer1 通道2映射口P1_0
        ORL     0xf3,#0x80
//  235     
//  236     T1CC3H = 0x00;           //20%占空比为200us
        MOV     DPTR,#0x62ad
        CLR     A
        MOVX    @DPTR,A
//  237     T1CC3L = 0x00;           //修改T1CC2L可调整led的亮度
        MOV     DPTR,#0x62ac
        MOVX    @DPTR,A
//  238     T1CC0H = 0x00;           //1ms的周期时钟,频率为976.516HZ
        MOV     0xdb,#0x0
//  239     T1CC0L = 0x00; 
        MOV     0xda,#0x0
//  240     T1CCTL3 = 0x34;          // 模式选择 通道2比较模式
        MOV     DPTR,#0x62a3
        MOV     A,#0x34
        MOVX    @DPTR,A
//  241     T1CTL |= 0x03;            //250KHz 1分频
        ORL     0xe4,#0x3
//  242   
//  243 }
        POP     DPH
        CFI DPH0 SameValue
        CFI CFA_SP SP+-4
        POP     DPL
        CFI DPL0 SameValue
        CFI CFA_SP SP+-3
        LJMP    ?BRET
        CFI EndBlock cfiBlock0
        REQUIRE CLKCONCMD
        REQUIRE CLKCONSTA
        REQUIRE PERCFG
        REQUIRE P2SEL
        REQUIRE P2DIR
        REQUIRE P0DIR
        REQUIRE P0SEL
        REQUIRE T1CC0H
        REQUIRE T1CC0L
        REQUIRE T1CTL
//  244 

        RSEG BANKED_CODE:CODE:NOROOT(0)
//  245 void InitT3(void)
InitT3:
        CFI Block cfiBlock1 Using cfiCommon0
        CFI Function InitT3
        CODE
//  246 {     
        ; Saved register size: 0
        ; Auto size: 0
//  247     T3CTL |= 0x08 ;          //开溢出中断     
        ORL     0xcb,#0x8
//  248     T3IE = 1;                //开总中断和T3中断
        SETB    0xb8.3
//  249     T3CTL |= 0xE0;           //128分频,128/16000000*N=0.5S,N=62500
        ORL     0xcb,#0xe0
//  250     T3CTL &= ~0x03;          //自动重装 00－>0xff  62500/255=245(次)
        ANL     0xcb,#0xfc
//  251     T3CTL |= 0x10;           //启动
        ORL     0xcb,#0x10
//  252     EA = 1;                  //开总中断
        SETB    0xa8.7
//  253 }
        LJMP    ?BRET
        CFI EndBlock cfiBlock1
        REQUIRE T3CTL
        REQUIRE _A_IEN1
        REQUIRE _A_IEN0
//  254 #pragma vector = T3_VECTOR 

        RSEG NEAR_CODE:CODE:NOROOT(0)
//  255 __interrupt void T3_ISR(void) 
T3_ISR:
        CFI Block cfiBlock2 Using cfiCommon1
        CFI Function T3_ISR
        CODE
