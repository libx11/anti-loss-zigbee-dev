//  201 #define BEEP P0_7  //蜂鸣器；
//  202 
//  203 
//  204 
//  205 /****************************************************************************
//  206 * 名    称: InitLed()
//  207 * 功    能: 设置LED灯相应的IO口
//  208 * 入口参数: 无
//  209 * 出口参数: 无
//  210 ****************************************************************************/
//  211 
//  212 /****************************************************************************
//  213 * 名    称: InitT1()
//  214 * 功    能: 定时器初始化，TICKSPD 是16 MHz系统不配置时默认是2分频，即16MHz
//  215 * 入口参数: 无
//  216 * 出口参数: 无
//  217 ****************************************************************************/
//  218 
//  219 

        RSEG BANKED_CODE:CODE:NOROOT(0)
//  220 void InitT1(void)
InitT1:
        CFI Block cfiBlock0 Using cfiCommon0
        CFI Function InitT1
        CODE
//  221 {
        PUSH    DPL
        CFI DPL0 Frame(CFA_SP, 4)
        CFI CFA_SP SP+-4
        PUSH    DPH
        CFI DPH0 Frame(CFA_SP, 5)
        CFI CFA_SP SP+-5
        ; Saved register size: 2
        ; Auto size: 0
//  222     CLKCONCMD &= ~0x40;      //设置系统时钟源为32MHZ晶振
        ANL     0xc6,#0xbf
//  223     while(CLKCONSTA & 0x40); //等待晶振稳定为32M
??InitT1_0:
        MOV     A,0x9e
        MOV     C,0xE0 /* A   */.6
        JC      ??InitT1_0
//  224     CLKCONCMD &= ~0x07;      //设置系统主时钟频率为32MHZ   
        ANL     0xc6,#0xf8
//  225     CLKCONCMD |= 0x38;       //时钟速度32 MHz 定时器标记输出设置[5:3]250kHz
        ORL     0xc6,#0x38
//  226 
//  227     PERCFG |= 0x40;          //定时器1 的IO位置   1:备用位置2 
        ORL     0xf1,#0x40
//  228     P2SEL &= ~0x10;          //定时器1优先
        ANL     0xf5,#0xef
//  229     P2DIR |= 0xC0;           //第1优先级：定时器1通道2-3
        ORL     0xff,#0xc0
//  230 
//  231     P0DIR |= 0x80;           //端口1为输出    
        ORL     0xfd,#0x80
//  232     P0SEL |= 0x80;           //timer1 通道2映射口P1_0
        ORL     0xf3,#0x80
//  233     
//  234     T1CC3H = 0x00;           //20%占空比为200us
        MOV     DPTR,#0x62ad
        CLR     A
        MOVX    @DPTR,A
//  235     T1CC3L = 0x00;           //修改T1CC2L可调整led的亮度
        MOV     DPTR,#0x62ac
        MOVX    @DPTR,A
//  236     T1CC0H = 0x00;           //1ms的周期时钟,频率为976.516HZ
        MOV     0xdb,#0x0
//  237     T1CC0L = 0x00; 
        MOV     0xda,#0x0
//  238     T1CCTL3 = 0x34;          // 模式选择 通道2比较模式
        MOV     DPTR,#0x62a3
        MOV     A,#0x34
        MOVX    @DPTR,A
//  239     T1CTL |= 0x03;            //250KHz 1分频
        ORL     0xe4,#0x3
//  240   
//  241 }
        POP     DPH
        CFI DPH0 SameValue
        CFI CFA_SP SP+-4
        POP     DPL
        CFI DPL0 SameValue
        CFI CFA_SP SP+-3
        LJMP    ?BRET
        CFI EndBlock cfiBlock0
        REQUIRE CLKCONCMD
        REQUIRE CLKCONSTA
        REQUIRE PERCFG
        REQUIRE P2SEL
        REQUIRE P2DIR
        REQUIRE P0DIR
        REQUIRE P0SEL
        REQUIRE T1CC0H
        REQUIRE T1CC0L
        REQUIRE T1CTL
//  242 

        RSEG BANKED_CODE:CODE:NOROOT(0)
//  243 void InitT3(void)
InitT3:
        CFI Block cfiBlock1 Using cfiCommon0
        CFI Function InitT3
        CODE
//  244 {     
        ; Saved register size: 0
        ; Auto size: 0
//  245     T3CTL |= 0x08 ;          //开溢出中断     
        ORL     0xcb,#0x8
//  246     T3IE = 1;                //开总中断和T3中断
        SETB    0xb8.3
