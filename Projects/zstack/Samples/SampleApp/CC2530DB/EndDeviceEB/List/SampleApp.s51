//  221 void InitT1(void)
InitT1:
        CFI Block cfiBlock0 Using cfiCommon0
        CFI Function InitT1
        CODE
//  222 {
        PUSH    DPL
        CFI DPL0 Frame(CFA_SP, 4)
        CFI CFA_SP SP+-4
        PUSH    DPH
        CFI DPH0 Frame(CFA_SP, 5)
        CFI CFA_SP SP+-5
        ; Saved register size: 2
        ; Auto size: 0
//  223     CLKCONCMD &= ~0x40;      //设置系统时钟源为32MHZ晶振
        ANL     0xc6,#0xbf
//  224     while(CLKCONSTA & 0x40); //等待晶振稳定为32M
??InitT1_0:
        MOV     A,0x9e
        MOV     C,0xE0 /* A   */.6
        JC      ??InitT1_0
//  225     CLKCONCMD &= ~0x07;      //设置系统主时钟频率为32MHZ   
        ANL     0xc6,#0xf8
//  226     CLKCONCMD |= 0x38;       //时钟速度32 MHz 定时器标记输出设置[5:3]250kHz
        ORL     0xc6,#0x38
//  227 
//  228     PERCFG |= 0x40;          //定时器1 的IO位置   1:备用位置2 
        ORL     0xf1,#0x40
//  229     P2SEL &= ~0x10;          //定时器1优先
        ANL     0xf5,#0xef
//  230     P2DIR |= 0xC0;           //第1优先级：定时器1通道2-3
        ORL     0xff,#0xc0
//  231 
//  232     P0DIR |= 0x80;           //端口1为输出    
        ORL     0xfd,#0x80
//  233     P0SEL |= 0x80;           //timer1 通道2映射口P1_0
        ORL     0xf3,#0x80
//  234     
//  235     T1CC3H = 0x00;           //20%占空比为200us
        MOV     DPTR,#0x62ad
        CLR     A
        MOVX    @DPTR,A
//  236     T1CC3L = 0x00;           //修改T1CC2L可调整led的亮度
        MOV     DPTR,#0x62ac
        MOVX    @DPTR,A
//  237     T1CC0H = 0x00;           //1ms的周期时钟,频率为976.516HZ
        MOV     0xdb,#0x0
//  238     T1CC0L = 0x00; 
        MOV     0xda,#0x0
//  239     T1CCTL3 = 0x34;          // 模式选择 通道2比较模式
        MOV     DPTR,#0x62a3
        MOV     A,#0x34
        MOVX    @DPTR,A
//  240     T1CTL |= 0x03;            //250KHz 1分频
        ORL     0xe4,#0x3
//  241   
//  242 }
        POP     DPH
        CFI DPH0 SameValue
        CFI CFA_SP SP+-4
        POP     DPL
        CFI DPL0 SameValue
        CFI CFA_SP SP+-3
        LJMP    ?BRET
        CFI EndBlock cfiBlock0
